          {
            "expr": "lead_time_metric_query"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "stat",
        "title": "Deployment Frequency",
        "targets": [
          {
            "expr": "deploy_frequency_query"
          }
