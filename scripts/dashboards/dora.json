        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "gauge",
        "title": "Change Failure Rate",
        "targets": [
          {
            "expr": "change_failure_query"
          }
