        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "stat",
        "title": "Time to Restore",
        "targets": [
          {
            "expr": "time_to_restore_query"
          }
